<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- Configuration of global properties -->
    <settings>
        <!-- Use method getGeneratedKeys in jdbc to obtain the auto-increment value in database -->
        <setting name="useGeneratedKeys" value="true"/>
        <!-- Replace column name by column label, default true
         E.g. select name as title from table -->
        <setting name="useColumnLabel" value="true"/>
        <!-- CamelCase switch on: Table(Create_time) -> Entity(createTime) -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
</configuration>